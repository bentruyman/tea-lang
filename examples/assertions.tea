use assert = "std.assert"
use debug = "std.debug"
use util = "std.util"

var words = ["tea", "lang"]

assert.assert(util.len(words) == 2, "expected two elements")
assert.assert_eq(true, util.is_bool(true))
assert.assert_ne(1, 2)

debug.print("len of 'tea':")
debug.print(util.len("tea"))
debug.print("clamped:")
debug.print(util.clamp_int(12, 0, 5))
debug.print("as string:")
debug.print(util.to_string(words))
debug.print("is struct?:")
debug.print(util.is_struct(words))
