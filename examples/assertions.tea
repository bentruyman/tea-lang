use assert = "std.assert"
use print = "std.print"
use util = "std.util"

var words = ["tea", "lang"]

assert.assert(util.len(words) == 2, "expected two elements")
assert.assert_eq(true, util.is_bool(true))
assert.assert_ne(1, 2)

print.print("len of 'tea':")
print.print(util.len("tea"))
print.print("clamped:")
print.print(util.clamp_int(12, 0, 5))
print.print("as string:")
print.print(util.to_string(words))
print.print("is struct?:")
print.print(util.is_struct(words))
