use debug = "std.debug"

## A user account with annotated fields
struct User {
  ## The user's display name
  name: String
  ## How many years old the user is
  age: Int
}

def print_user(user: User) -> Void
  debug.print(user.name)
  debug.print(user.age)
end

var ada = User(name: "Ada", age: 37)
print_user(ada)
