use assert = "std.assert"
use util = "std.util"

var words = ["tea", "lang"]

assert.assert(util.len(words) == 2, "expected two elements")
assert.assert_eq(true, util.is_bool(true))
assert.assert_ne(1, 2)

print("len of 'tea':")
print(util.len("tea"))
print("clamped:")
print(util.clamp_int(12, 0, 5))
print("as string:")
print(util.to_string(words))
print("is struct?:")
print(util.is_struct(words))
