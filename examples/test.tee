var x = 0, y = 0

def move
  x++, y++
end

print move # 1, 1

def hello_world(location = "World")
  "Hello #{location}"
end

def hello_world(location: String = "World")
  "Hello #{location}"
end

print hello_world # "Hello World"

def fib(n: Int)
  return n if n <= 1
  fib(n - 1) + fib(n - 2)
end

for i in 0..10
  print i
end

print fib(10)


