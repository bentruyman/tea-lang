{
  "name": "tree-sitter-tea",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for the Tea programming language.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bentruyman/tea-lang"
  },
  "keywords": [
    "parser",
    "tree-sitter",
    "tea"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "license": "MIT",
  "scripts": {
    "generate": "tree-sitter generate",
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "devDependencies": {
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.25.8"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "types": "bindings/node",
  "tree-sitter": [
    {
      "scope": "source.tea",
      "injection-regex": "^tea$"
    }
  ]
}
